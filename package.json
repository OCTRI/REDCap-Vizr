{
  "name": "redcap-vizr",
  "version": "2.2.2",
  "description": "Visualization tool for REDCap",
  "main": "./dist/bundle.js",
  "scripts": {
    "analyze": "webpack --mode=production; webpack --profile --json > bundle-stats.json; webpack-bundle-analyzer bundle-stats.json dist",
    "build-dev": "webpack --mode=development",
    "build": "webpack --mode=production",
    "clean": "rm -rf dist junit bundle-stats.json",
    "lint:js": "eslint ./*.js js/**/*.{js,vue} test/**/*.js",
    "prebuild": "npm install",
    "start": "WATCHING=1 webpack --mode=development --progress --colors --watch",
    "test-jenkins": "karma start karma.conf.js --single-run --reporters junit --browsers Chrome_Headless",
    "test-single-run": "karma start karma.conf.js --single-run",
    "test": "karma start karma.conf.js --single-run"
  },
  "author": "BMIP - OCTRI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://octriinternal.ohsu.edu/stash/scm/reddev/vizr.git"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0",
    "chart.js": "^2.7.3",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "eslint": "~5.9.0",
    "eslint-plugin-vue": "^5.0.0-beta.4",
    "file-loader": "~2.0.0",
    "git-repo-info": "^2.0.0",
    "html-webpack-plugin": "4.0.0-beta.4",
    "jasmine-ajax": "^3.4.0",
    "jasmine-core": "^3.3.0",
    "jquery": "~3.3.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^2.0.1",
    "karma-junit-reporter": "^1.2.0",
    "karma-spec-reporter": "~0.0.32",
    "karma-webpack": "^4.0.0-rc.3",
    "mini-css-extract-plugin": "^0.4.5",
    "moment": "^2.22.2",
    "prettier": "^1.15.2",
    "style-loader": "^0.23.1",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "qs": "^6.5.2",
    "striptags": "^3.1.1",
    "uuid": "^3.3.2",
    "vue": "^2.5.17"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
