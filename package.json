{
  "name": "redcap-vizr",
  "version": "2.2.1",
  "description": "Visualization tool for REDCap",
  "main": "./dist/bundle.js",
  "scripts": {
    "analyze": "webpack --optimize-minimize; webpack --profile --json > bundle-stats.json; webpack-bundle-analyzer bundle-stats.json dist",
    "clean": "rm -rf dist junit bundle-stats.json",
    "prebuild": "npm install",
    "build": "webpack --optimize-minimize",
    "build-dev": "webpack",
    "test": "karma start karma.conf.js --single-run",
    "test-jenkins": "karma start karma.conf.js --single-run --reporters junit --browsers Chrome_Headless",
    "test-single-run": "karma start karma.conf.js --single-run",
    "start": "WATCHING=1 webpack --progress --colors --watch"
  },
  "author": "BMIP - OCTRI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://octriinternal.ohsu.edu/stash/scm/reddev/vizr.git"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.19",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "~1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "chart.js": "^2.7.2",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "~4.15.0",
    "eslint-plugin-vue": "^4.5.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "~1.1.5",
    "git-repo-info": "^1.4.1",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-ajax": "^3.3.1",
    "jasmine-core": "^2.8.0",
    "jquery": "~1.12.0",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^3.0.2",
    "moment": "^2.19.3",
    "style-loader": "^0.21.0",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.12.0",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.10",
    "qs": "^6.5.2",
    "striptags": "^2.2.1",
    "uuid": "^3.1.0",
    "vue": "^2.5.16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
